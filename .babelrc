{
  "presets": ["next/babel"],
  "plugins": [    
  [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js", {
        "extensions": ["css$", "scss$"]
      }
    ],
    ["@babel/plugin-proposal-optional-chaining"],
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    // ["import", {"libraryName":  "antd", "style": true}]
  ]
}